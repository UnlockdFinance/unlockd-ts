{
  "name": "@unlockdfinance/unlockd-ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "version": "0.0.1",
  "description": "",
  "main": "dist/unlockd-ts.cjs",
  "module": "dist/unlockd-ts.esm",
  "browser": "dist/unlockd-ts.umd",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "node --experimental-json-modules ./node_modules/.bin/rollup --config rollup.config.js -w",
    "build": "npm run build:clean && npm run build:code && npm run build:types",
    "build:code": "node --experimental-json-modules ./node_modules/.bin/rollup --config rollup.config.js",
    "build:types": "tsc -p ./tsconfig.json --outDir build --declaration true && api-extractor run",
    "build:clean": "rm -r build; rm -r dist;exit 0",
    "docs": "typedoc src --out docs",
    "deploy": "npm run build && npm publish",
    "test": "jest",
    "format": "prettier --write \"src/**/*.+(js|ts|json|yaml)\" \"tests/**/*.+(js|ts|json|yaml)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UnlockdFinance/unlockd-ts.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/UnlockdFinance/unlockd-ts/issues"
  },
  "homepage": "https://github.com/UnlockdFinance/unlockd-ts#readme",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.36.4",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/jest": "^29.5.4",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "nock": "^13.3.3",
    "prettier": "^3.0.3",
    "rollup": "^3.29.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "joi": "^17.10.2"
  }
}
