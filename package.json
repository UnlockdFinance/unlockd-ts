{
  "name": "@unlockdfinance/unlockd-ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "version": "0.0.2",
  "description": "",
  "main": "dist/unlockd-ts.cjs",
  "module": "dist/unlockd-ts.esm",
  "browser": "dist/unlockd-ts.umd",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "node --experimental-json-modules ./node_modules/.bin/rollup --config rollup.config.js -w",
    "build": "npm run build:clean && npm run build:code && npm run build:types",
    "build:code": "node --experimental-json-modules ./node_modules/.bin/rollup --config rollup.config.js",
    "build:types": "tsc -p ./tsconfig-build.json --outDir build --declaration true && api-extractor run",
    "build:clean": "rm -r build; rm -r dist;exit 0",
    "docs": "typedoc --plugin typedoc-plugin-markdown  --out docs src/index.ts ",
    "deploy": "npm run build && npm publish",
    "test": "jest",
    "changelog": " npx auto-changelog --output CHANGELOG.md --template keepachangelog --commit-limit false --package",
    "format": "prettier --write \"src/**/*.+(js|ts|json|yaml)\" \"tests/**/*.+(js|ts|json|yaml)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UnlockdFinance/unlockd-ts.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/UnlockdFinance/unlockd-ts/issues"
  },
  "homepage": "https://github.com/UnlockdFinance/unlockd-ts#readme",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.38.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.4",
    "@types/jest": "^29.5.5",
    "acorn-import-assertions": "^1.9.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.50.0",
    "ethers": "^6.8.1",
    "husky": "^9.0.7",
    "jest": "^29.7.0",
    "nock": "^13.3.3",
    "prettier": "^3.0.3",
    "rollup": "^3.29.4",
    "rollup-plugin-import-assert": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.7",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "joi": "^17.10.2"
  }
}
